{
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "build/",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/products/api",
      "dest": "index.js"
    },
    {
      "src": "/products/api//(.*)",
      "dest": "index.js"
    },
    {
      "src": "/(.*)",
      "dest": "build/"
    }
  ]
}
